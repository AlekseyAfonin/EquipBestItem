<Prefab>
	<Constants>
		<Constant Name="EBI.MainWindow.MarginTop" Value="0"/>
		<Constant Name="EBI.Slider.Width" Value="160"/>
		<Constant Name="EBI.Slider.Height" Value="55"/>
		<Constant Name="EBI.Slider.MarginLeft" Value="20"/>
		<Constant Name="EBI.Slider.Value.MarginLeft" Value="20"/>
		<Constant Name="EBI.CheckBox.MarginLeft" Value="10"/>
	</Constants>
	<Window>
		<Widget DoNotAcceptEvents="true" WidthSizePolicy="StretchToParent" HeightSizePolicy="StretchToParent">
			<Children>
				<!-- Border -->
				<BrushWidget DoNotAcceptEvents="false" Id="Panel" WidthSizePolicy="Fixed" HeightSizePolicy="Fixed" SuggestedWidth="470" SuggestedHeight="780" HorizontalAlignment="Center" VerticalAlignment="Top" MarginTop="200" Brush="Frame1Brush">
					<Children>
						<!-- Main List -->
						<ListPanel WidthSizePolicy="StretchToParent" HeightSizePolicy="CoverChildren" HorizontalAlignment="Left" VerticalAlignment="Top" LayoutImp.LayoutMethod="VerticalBottomToTop">
							<Children>
								<!-- Header -->
								<ListPanel WidthSizePolicy="StretchToParent" HeightSizePolicy="CoverChildren" HorizontalAlignment="Center" VerticalAlignment="Top" LayoutImp.LayoutMethod="VerticalBottomToTop" MarginTop="0" Sprite="BlankWhiteSquare_9" Color="#8B5C2200">
									<Children>
										<RichTextWidget DoNotAcceptEvents="true" WidthSizePolicy="CoverChildren" HeightSizePolicy="CoverChildren" HorizontalAlignment="Center" VerticalAlignment="Bottom" MarginTop="0" Text="@HeaderText"/>
									</Children>
								</ListPanel>
								<!-- List Row Begin-->
								<ListPanel WidthSizePolicy="StretchToParent" HeightSizePolicy="CoverChildren" HorizontalAlignment="Center" VerticalAlignment="Top" LayoutImp.LayoutMethod="HorizontalLeftToRight" MarginTop="!EBI.MainWindow.MarginTop" Sprite="BlankWhiteSquare_9" Color="#8B5C2200">
									<Children>
										<!-- Left Column -->
										<ListPanel Id="LeftListPanel" DoNotAcceptEvents="true" WidthSizePolicy="Fixed" SuggestedWidth="200" HeightSizePolicy="CoverChildren" HorizontalAlignment="Left" VerticalAlignment="Top" MarginLeft="0" MarginRight="0" LayoutImp.LayoutMethod="HorizontalLeftToRight" Sprite="BlankWhiteSquare_9" Color="#8B5C2200">
											<Children>
												<Widget DoNotAcceptEvents="true" WidthSizePolicy="StretchToParent" HeightSizePolicy="Fixed" SuggestedHeight="!EBI.Slider.Height" VerticalAlignment="Center" MarginLeft="!EBI.Slider.MarginLeft">
													<Children>
														<ListPanel Id="NumericOption" WidthSizePolicy="CoverChildren" HeightSizePolicy="CoverChildren" HorizontalAlignment="Left" VerticalAlignment="Center" IsVisible="true">
															<Children>
																<RichTextWidget WidthSizePolicy="StretchToParent" HeightSizePolicy="CoverChildren" MarginRight="0" VerticalAlignment="Center" Brush="SPOptions.OptionName.Text" IsEnabled="false" Text="@HeaderText"/>
															</Children>
														</ListPanel>
													</Children>
												</Widget>
											</Children>
										</ListPanel>
										<!-- Right Column -->
										<ListPanel Id="RightListPanel" DoNotAcceptEvents="true" WidthSizePolicy="StretchToParent" HeightSizePolicy="CoverChildren" HorizontalAlignment="Right" VerticalAlignment="Top" MarginLeft="0" LayoutImp.LayoutMethod="HorizontalLeftToRight" Sprite="BlankWhiteSquare_9" Color="#3B5C2200">
											<Children>
												<Widget Id="Option" DoNotAcceptEvents="true" WidthSizePolicy="StretchToParent" HeightSizePolicy="Fixed" SuggestedHeight="!EBI.Slider.Height" VerticalAlignment="Center" MarginLeft="!EBI.Slider.MarginLeft">
													<Children>
														<ListPanel Id="NumericOption" WidthSizePolicy="CoverChildren" HeightSizePolicy="CoverChildren" HorizontalAlignment="Left" VerticalAlignment="Center" IsVisible="true">
															<Children>
																<!-- Slider -->
																<SliderWidget WidthSizePolicy="Fixed" HeightSizePolicy="Fixed" SuggestedWidth="138" SuggestedHeight="42" VerticalAlignment="Center" DoNotUpdateHandleSize="true" Filler="Filler" Handle="SliderHandle" IsDiscrete="true" MaxValueFloat="100.0" MinValueFloat="0.0" ValueFloat="@HeadArmorValue">
																	<Children>
																		<Widget WidthSizePolicy="Fixed" HeightSizePolicy="Fixed" SuggestedWidth="138" SuggestedHeight="38" HorizontalAlignment="Center" VerticalAlignment="Center" Sprite="SPGeneral\SPOptions\standart_slider_canvas" IsEnabled="false"/>
																		<Widget Id="Filler" WidthSizePolicy="Fixed" HeightSizePolicy="Fixed" SuggestedWidth="345" SuggestedHeight="35" VerticalAlignment="Center" Sprite="SPGeneral\SPOptions\standart_slider_fill" ClipContents="true">
																			<Children>
																				<Widget WidthSizePolicy="Fixed" HeightSizePolicy="Fixed" SuggestedWidth="345" SuggestedHeight="35" HorizontalAlignment="Left" VerticalAlignment="Center" Sprite="SPGeneral\SPOptions\standart_slider_fill"/>
																			</Children>
																		</Widget>
																		<Widget WidthSizePolicy="Fixed" HeightSizePolicy="Fixed" SuggestedWidth="!EBI.Slider.Width" SuggestedHeight="65" HorizontalAlignment="Center" VerticalAlignment="Center" Sprite="SPGeneral\SPOptions\standart_slider_frame" IsEnabled="false"/>
																		<ImageWidget Id="SliderHandle" DoNotAcceptEvents="true" WidthSizePolicy="Fixed" HeightSizePolicy="Fixed" SuggestedWidth="14" SuggestedHeight="38" HorizontalAlignment="Left" VerticalAlignment="Center" Brush="SPOptions.Slider.Handle"/>
																	</Children>
																</SliderWidget>
																<RichTextWidget WidthSizePolicy="Fixed" SuggestedWidth="50" HeightSizePolicy="CoverChildren" MarginLeft="0" VerticalAlignment="Center" Brush="SPOptions.OptionName.Text" IsEnabled="false" Text="@HeadArmorValueText"/>
																<!--Default CheckBox-->
																<ButtonWidget DoNotPassEventsToChildren="true" WidthSizePolicy="Fixed" HeightSizePolicy="Fixed" SuggestedWidth="40" SuggestedHeight="40" MarginLeft="!EBI.CheckBox.MarginLeft" Brush="SPOptions.Checkbox.Empty.Button" VerticalAlignment="Center" HorizontalAlignment="Right" ButtonType="Toggle" Command.Click="ExecuteSwingDamageDefault" IsSelected="@SwingDamageIsDefault" ToggleIndicator="ToggleIndicator" UpdateChildrenStates="true">
																	<Children>
																		<ImageWidget Id="ToggleIndicator" WidthSizePolicy="StretchToParent" HeightSizePolicy="StretchToParent" Brush="SPOptions.Checkbox.Full.Button"/>
																	</Children>
																</ButtonWidget>
															</Children>
														</ListPanel>
													</Children>
												</Widget>
											</Children>
										</ListPanel>
									</Children>
								</ListPanel>
								<!-- List Row End-->
								<!-- List Row Begin-->
								<ListPanel WidthSizePolicy="StretchToParent" HeightSizePolicy="CoverChildren" HorizontalAlignment="Center" VerticalAlignment="Top" LayoutImp.LayoutMethod="HorizontalLeftToRight" MarginTop="!EBI.MainWindow.MarginTop" Sprite="BlankWhiteSquare_9" Color="#8B5C2200">
									<Children>
										<!-- Left Column -->
										<ListPanel Id="LeftListPanel" DoNotAcceptEvents="true" WidthSizePolicy="Fixed" SuggestedWidth="200" HeightSizePolicy="CoverChildren" HorizontalAlignment="Left" VerticalAlignment="Top" MarginLeft="0" MarginRight="0" LayoutImp.LayoutMethod="HorizontalLeftToRight" Sprite="BlankWhiteSquare_9" Color="#8B5C2200">
											<Children>
												<Widget DoNotAcceptEvents="true" WidthSizePolicy="StretchToParent" HeightSizePolicy="Fixed" SuggestedHeight="!EBI.Slider.Height" VerticalAlignment="Center" MarginLeft="!EBI.Slider.MarginLeft">
													<Children>
														<ListPanel Id="NumericOption" WidthSizePolicy="CoverChildren" HeightSizePolicy="CoverChildren" HorizontalAlignment="Left" VerticalAlignment="Center" IsVisible="true">
															<Children>
																<RichTextWidget WidthSizePolicy="StretchToParent" HeightSizePolicy="CoverChildren" MarginRight="0" VerticalAlignment="Center" Brush="SPOptions.OptionName.Text" IsEnabled="false" Text="Head armor"/>
															</Children>
														</ListPanel>
													</Children>
												</Widget>
											</Children>
										</ListPanel>
										<!-- Right Column -->
										<ListPanel Id="RightListPanel" DoNotAcceptEvents="true" WidthSizePolicy="StretchToParent" HeightSizePolicy="CoverChildren" HorizontalAlignment="Right" VerticalAlignment="Top" MarginLeft="0" LayoutImp.LayoutMethod="HorizontalLeftToRight" Sprite="BlankWhiteSquare_9" Color="#3B5C2200">
											<Children>
												<Widget Id="Option" DoNotAcceptEvents="true" WidthSizePolicy="StretchToParent" HeightSizePolicy="Fixed" SuggestedHeight="!EBI.Slider.Height" VerticalAlignment="Center" MarginLeft="!EBI.Slider.MarginLeft">
													<Children>
														<ListPanel Id="NumericOption" WidthSizePolicy="CoverChildren" HeightSizePolicy="CoverChildren" HorizontalAlignment="Left" VerticalAlignment="Center" IsVisible="true">
															<Children>
																<!-- Slider -->
																<SliderWidget WidthSizePolicy="Fixed" HeightSizePolicy="Fixed" SuggestedWidth="138" SuggestedHeight="42" VerticalAlignment="Center" DoNotUpdateHandleSize="true" Filler="Filler" Handle="SliderHandle" IsDiscrete="true" MaxValueFloat="100.0" MinValueFloat="0.0" ValueFloat="@HeadArmorValue">
																	<Children>
																		<Widget WidthSizePolicy="Fixed" HeightSizePolicy="Fixed" SuggestedWidth="138" SuggestedHeight="38" HorizontalAlignment="Center" VerticalAlignment="Center" Sprite="SPGeneral\SPOptions\standart_slider_canvas" IsEnabled="false"/>
																		<Widget Id="Filler" WidthSizePolicy="Fixed" HeightSizePolicy="Fixed" SuggestedWidth="345" SuggestedHeight="35" VerticalAlignment="Center" Sprite="SPGeneral\SPOptions\standart_slider_fill" ClipContents="true">
																			<Children>
																				<Widget WidthSizePolicy="Fixed" HeightSizePolicy="Fixed" SuggestedWidth="345" SuggestedHeight="35" HorizontalAlignment="Left" VerticalAlignment="Center" Sprite="SPGeneral\SPOptions\standart_slider_fill"/>
																			</Children>
																		</Widget>
																		<Widget WidthSizePolicy="Fixed" HeightSizePolicy="Fixed" SuggestedWidth="!EBI.Slider.Width" SuggestedHeight="65" HorizontalAlignment="Center" VerticalAlignment="Center" Sprite="SPGeneral\SPOptions\standart_slider_frame" IsEnabled="false"/>
																		<ImageWidget Id="SliderHandle" DoNotAcceptEvents="true" WidthSizePolicy="Fixed" HeightSizePolicy="Fixed" SuggestedWidth="14" SuggestedHeight="38" HorizontalAlignment="Left" VerticalAlignment="Center" Brush="SPOptions.Slider.Handle"/>
																	</Children>
																</SliderWidget>
																<RichTextWidget WidthSizePolicy="Fixed" SuggestedWidth="50" HeightSizePolicy="CoverChildren" MarginLeft="0" VerticalAlignment="Center" Brush="SPOptions.OptionName.Text" IsEnabled="false" Text="@HeadArmorValueText"/>
																<!--Default CheckBox-->
																<ButtonWidget DoNotPassEventsToChildren="true" WidthSizePolicy="Fixed" HeightSizePolicy="Fixed" SuggestedWidth="40" SuggestedHeight="40" MarginLeft="!EBI.CheckBox.MarginLeft" Brush="SPOptions.Checkbox.Empty.Button" VerticalAlignment="Center" HorizontalAlignment="Right" ButtonType="Toggle" Command.Click="ExecuteSwingDamageDefault" IsSelected="@SwingDamageIsDefault" ToggleIndicator="ToggleIndicator" UpdateChildrenStates="true">
																	<Children>
																		<ImageWidget Id="ToggleIndicator" WidthSizePolicy="StretchToParent" HeightSizePolicy="StretchToParent" Brush="SPOptions.Checkbox.Full.Button"/>
																	</Children>
																</ButtonWidget>
															</Children>
														</ListPanel>
													</Children>
												</Widget>
											</Children>
										</ListPanel>
									</Children>
								</ListPanel>
								<!-- List Row End-->
							</Children>
						</ListPanel>
					</Children>
				</BrushWidget>
			</Children>
		</Widget>
	</Window>
</Prefab>